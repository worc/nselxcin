{
  "name": "nselxcin",
  "version": "0.0.0",
  "description": "interior salish curriculum",
  "main": "server.js",
  "repository": "https://github.com/worc/nselxcin",
  "author": "curtis mcallister",
  "license": "MIT",
  "engines": {
    "node": "^14.15.4"
  },
  "scripts": {
    "dev": "webpack-dev-server --port 8080 --progress",
    "db:dump": "pg_dump -U nselxcin -d nselxcin-test -h localhost -p 5432 --format plain --file postgres/dump.sql",
    "start": "nodemon server.js --exec babel-node",
    "start:api": "nodemon db/api.js --exec babel-node",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "parcel": "parcel server.js",
    "test": "mocha --require @babel/register --recursive test",
    "watch": "webpack --config webpack.config.js --watch",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "express": "^4.16.3",
    "prop-types": "^15.6.1",
    "pg": "^8.5.1",
    "pg-native": "^3.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "sequelize": "^4.37.6",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.13",
    "@storybook/react": "^6.1.16",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.5.1",
    "chai": "^4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^0.28.11",
    "mocha": "^5.2.0",
    "nodemon": "^2.0.7",
    "parcel-bundler": "^1.7.1",
    "sequelize-cli": "^4.0.0",
    "sinon": "^6.1.4",
    "style-loader": "^0.21.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^1.7.2"
  },
  "peerDependencies": {
    "html-webpack-plugin": "^5.0.0"
  }
